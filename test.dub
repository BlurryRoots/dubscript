c : false {
    ("keks" < depp_kopf) : {
        def! stuff "nudelsalat";
    } 
    : depp_kopf = "hans" {
        def! hans (index value) -> {
            // create a list
            def! keks [12 3 42];
            // return result of arithmic expression
            return keks * keks
                 + 3 * keks * (0xAFFE42 + 1.618);
        };
        def! blank () -> {};
    };

    (-2.3 \= 13) : {
        def? stuff;
    };
};